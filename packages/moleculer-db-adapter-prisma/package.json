{
  "name": "moleculer-db-adapter-prisma",
  "version": "0.1.16",
  "description": "Prisma adapter for Moleculer DB service",
  "main": "index.js",
  "types": "./index.d.ts",
  "scripts": {
    "prisma:gen": "prisma generate",
    "prisma:push": "prisma db push",
    "dev": "nodemon examples/index.js",
    "ci": "jest --watch",
    "test": "jest --coverage",
    "lint": "eslint --ext=.js src test",
    "deps": "npm-check -u",
    "readme": "node node_modules/moleculer-docgen/index.js -t README.md src/index.js",
    "coverall": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "keywords": [
    "microservice",
    "moleculer",
    "orm",
    "database",
    "prisma"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:moleculerjs/moleculer-addons.git"
  },
  "homepage": "https://github.com/moleculerjs/moleculer-db/tree/master/packages/moleculer-db-adapter-prisma#readme",
  "author": "MoleculerJS",
  "license": "MIT",
  "peerDependencies": {
    "moleculer": "^0.12.0 || ^0.13.0 || ^0.14.0",
    "prisma": "^4.2.1"
  },
  "dependencies": {
    "@prisma/client": "^4.2.1"
  },
  "devDependencies": {
    "benchmarkify": "2.1.0",
    "coveralls": "3.0.0",
    "eslint": "4.18.2",
    "jest": "22.4.2",
    "jest-cli": "22.4.2",
    "lolex": "2.3.2",
    "moleculer": "^0.14.23",
    "moleculer-docgen": "^0.3.0",
    "nodemon": "1.17.1",
    "npm-check": "5.5.2",
    "prisma": "^4.2.1"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/services/"
    ]
  },
  "engines": {
    "node": ">= 6.x.x"
  }
}
